---
layout: default.njk
---

{% include "page-title.njk" %}
<div class="container-fluid">
  {%- if eleventyNavigation.img %}
  <div class="row">
    <div class="col-md-6">
      <img alt="" src="{{ eleventyNavigation.img }}" class="img-fluid rounded mb-3" aria-hidden="true"/>
    </div>
  </div>
  {%- endif %}
  <div class="row">
    <div class="col-md-6">
      {% include "page-contents.njk" %}
    </div>
  </div>
</div>
<section class="content-container content-area" aria-label="Page Content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-12">
        {{ content | safe }}
      </div>
    </div>
  </div>
</section>
{% include "page-navigation.njk" %}
