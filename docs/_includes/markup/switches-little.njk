<div class="form-check form-switch">
    <input class="form-check-input" type="checkbox" role="switch" id="defaultSwitch">
    <label class="form-check-label" for="defaultSwitch">Default switch checkbox input</label>
</div>
<div class="form-check form-switch">
    <input class="form-check-input" type="checkbox" role="switch" id="checkedSwitch" checked>
    <label class="form-check-label" for="checkedSwitch">Checked switch checkbox input</label>
</div>
<div class="form-check form-switch">
    <input class="form-check-input" type="checkbox" role="switch" id="disabledSwitch" disabled>
    <label class="form-check-label" for="disabledSwitch">Disabled switch checkbox input</label>
</div>
<div class="form-check form-switch">
    <input class="form-check-input" type="checkbox" role="switch" id="disabledCheckedSwitch" checked disabled>
    <label class="form-check-label" for="disabledCheckedSwitch">Disabled checked switch checkbox input</label>
</div>
