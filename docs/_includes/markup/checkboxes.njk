<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="checkboxDefault">
  <label class="form-check-label" for="checkboxDefault">Default checkbox</label>
</div>
<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="checkedCheckbox" checked>
  <label class="form-check-label" for="checkedCheckbox">Checked checkbox</label>
</div>
<div class="form-check">
  <input class="form-check-input" type="checkbox" value="" id="disabledCheckbox" disabled>
  <label class="form-check-label" for="disabledCheckbox">Disabled checkbox</label>
</div>
